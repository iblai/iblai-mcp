# iblai-instructure Configuration
# Copy this file to .env and update with your values

# Base URL (optional, defaults to ibleducation.instructure.com)
# INSTRUCTURE_BASE_URL=https://ibleducation.instructure.com

# Canvas LMS uses Bearer token authentication
# Generate an access token from your Canvas account settings
INSTRUCTURE_AUTH_TYPE=bearer
INSTRUCTURE_BEARER_TOKEN=your_canvas_access_token
